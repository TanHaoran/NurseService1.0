	    SELECT * FROM StudySchedule
	</select>
    
    <select id="StudySchedule_Find" parameterClass="String" resultMap="FullResultMap" extends="StudySchedule_FindAll">
	    WHERE
            (PlanID = #PlanID#)
	</select>
    
