            <result property="OperatorID" column="OperatorID" dbType="String"/>
            <result property="ModifyDate" column="ModifyDate" dbType="DateTime"/>
    </resultMap>
  </resultMaps>

  <statements>
    <select id="StudySchedule_GetCount" resultClass="System.Int32">
